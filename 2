f = open('27-A.txt').readlines()
print(f)
a = []
b = []
for line in f[1:]:
    a.append(int(line.split()[0]))
for i in range(int(f[0])):
    for j in range(int(f[0])):
        for k in range(int(f[0])):
            if (a[i] + a[j] + a[k]) % 3 == 0 and a[i] != a[j] != a[k]:
                b.append(a[i] + a[j] + a[k])
print(min(b))
